<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data noupdate="0">
		<record id="auto_delivery_order" model="ir.cron">
			<field name="name">Delivery Order is Delivered</field>
			<field name="model_id" ref="stock.model_stock_picking" />
			<field name="state">code</field>
			<field name="code">model._cron_delivered_order()</field>
			<field name="interval_number">10</field>
			<field name="interval_type">minutes</field>
			<field name="nextcall"
      eval="(DateTime.now().replace(hour=0, minute=2) + timedelta(days=1)).strftime('%Y-%m-%d %H:%M:%S')"/>
			<field name="active" eval="False" />
		</record>
    </data>
</odoo>